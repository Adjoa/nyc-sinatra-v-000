<h1>Edit <%= @landmark.name %></h1>
<form method="post" action="/landmarks/<%= @landmark.id %>">
  <input type="hidden" name="_method" value="patch">
  <label>Name</label>
  <input type="text" id="landmark_name" name="landmark[name]" value="<%= @landmark.name %>">
  <br></br>

  <label>Year Completed</label>
  <input type="text" id="landmark_year_completed" name="landmark[year_completed]" value="<%= @landmark.year_completed.to_s %>">
  <br></br>

  <input type="submit" value="Edit Landmark">
</form>
